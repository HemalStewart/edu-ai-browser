#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/electronmon@2.0.4/node_modules/electronmon/bin/node_modules:/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/electronmon@2.0.4/node_modules/electronmon/node_modules:/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/electronmon@2.0.4/node_modules:/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/electronmon@2.0.4/node_modules/electronmon/bin/node_modules:/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/electronmon@2.0.4/node_modules/electronmon/node_modules:/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/electronmon@2.0.4/node_modules:/Users/lakminiinternational/edu-ai-browser/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../electronmon/bin/cli.js" "$@"
else
  exec node  "$basedir/../electronmon/bin/cli.js" "$@"
fi
